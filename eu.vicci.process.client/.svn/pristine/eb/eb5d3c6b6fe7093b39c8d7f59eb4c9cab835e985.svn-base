package eu.vicci.process.client.subscribers;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

import eu.vicci.process.model.util.messages.WampResponse;
import eu.vicci.process.model.util.messages.core.MessageType;
import ws.wamp.jawampa.PubSubData;

public class WampResponseSubscriper extends AbstractSubscriber<PubSubData>{
	private static final Logger logger = LoggerFactory.getLogger(WampMessageSubscriber.class);
	
	private WampResponse response;

	@Override
	public void onCompleted() {
		logger.debug("completed");				
	}

	@Override
	public void onError(Throwable e) {
		logger.error(e.getMessage());		
	}

	@Override
	public void onNext(PubSubData t) {
		response = convertFromJson(t.arguments().get(0), WampResponse.class);
		receiver.onMessage(MessageType.WAMPRESPONSE, response);
	}

}
